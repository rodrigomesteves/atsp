red = "#FF0000"; green = "#00FF00"; blue = "#0000FF"; skyblue = "#87CEEB"; yellow = "#FFFF00"; brown = "#8B4513"; pink = "#FF69B4";
set yrange [0:4000]
set xrange [-1:10]
set style data histogram
set style histogram cluster gap 1
set style fill solid
set boxwidth 0.9
set xtics format ""
set grid ytics

set title "Problemas x Resultados"
plot "/home/rodrigo/Documentos/UFF/relatorios/custo-all.dat" using 2:xtic(1) title "Otimo" ls 2 linecolor rgb skyblue, \
            '' using 4 title "Opt-2 MA" ls 2 linecolor rgb red, \
            '' using 5 title "Opt-2 PA" ls 2 linecolor rgb yellow, \
            '' using 6 title "Local Neighborhood MA" ls 2 linecolor rgb brown, \
            '' using 7 title "Local Neighborhood PA" ls 2 linecolor rgb pink, \
            '' using 3 title "Guloso" ls 2 linecolor rgb green








